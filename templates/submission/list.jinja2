
<table class="ui celled table center aligned compact unstackable">
    <thead>
        <tr>
            <th>提交号</th>
            <th>提交时间</th>
            <th>状态</th>
        </tr>
    </thead>
    <tbody>
        {%for obj in submissions%}
            <tr>
                <td><a  href="{{url('submission_detail', obj.pk)}}" >{{obj.pk}}</a></td>
                <td>{{obj.submit_time | date('Y-m-d H:i') }}</td>
                <td>{{ STATUS_STR[obj.status] }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>
